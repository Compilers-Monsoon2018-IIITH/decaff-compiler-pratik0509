class Program {
    int arr[50];
    int main() {
        int i = 0;
        for i = 1, 51 {
            arr[i-1] = (i * (i + 1) + i + 23) % 107;
        }

        int max1 = -1;
        int max2 = -1;
        for i = 0, 50 {
            if (max1 < arr[i]) {
                max2 = max1;
                max1 = arr[i];
            } else if (max2 < arr[i]) {
                max2 = arr[i];
            }
        }
        callout("printf", "max2 = %d\n", max2);
        return 0;
    }
}